{
  "all": true,
  "include": [
    "src/**/*.js"
  ],
  "exclude": [
    "src/tests/**",
    "src/server.js"
  ],
  "reporter": [
    "html",
    "text",
    "lcov",
    "json-summary"
  ],
  "report-dir": "coverage",
  "check-coverage": true,
  "lines": 80,
  "functions": 80,
  "branches": 80,
  "statements": 80,
  "temp-dir": ".nyc_output"
}
